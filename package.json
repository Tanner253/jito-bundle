{
  "name": "jito-bundler",
  "version": "1.0.0",
  "description": "Secure Jito bundler with maximum protection against losses",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:production": "ts-node src/test/production-validation.ts",
    "check-balance": "ts-node src/test/check-balance.ts",
    "production": "set NODE_ENV=production && ts-node src/index.ts",
    "dev:devnet": "set NODE_ENV=devnet && nodemon --exec ts-node src/index.ts",
    "dev:mainnet": "set NODE_ENV=mainnet && nodemon --exec ts-node src/index.ts",
    "api-server": "ts-node src/api/index.ts",
    "start-full": "concurrently \"npm run api-server\" \"cd frontend && npm run dev\"",
    "recover-funds": "ts-node scripts/recover-funds.ts",
    "simple-recovery": "ts-node scripts/simple-recovery.ts",
    "fund-and-recover": "ts-node scripts/simple-fund-recover.ts",
    "emergency-consolidate": "ts-node scripts/emergency-consolidate.ts",
    "test-token": "ts-node scripts/test-token.ts",
    "test-rpc": "ts-node scripts/test-rpc.ts",
    "fast-consolidate": "ts-node scripts/fast-consolidate.ts",
    "get-my-sol": "ts-node scripts/get-my-sol.ts",
    "detect-accounts": "ts-node scripts/detect-wallet-accounts.ts",
    "close-accounts": "ts-node scripts/close-empty-accounts.ts"
  },
  "dependencies": {
    "@solana/spl-token": "^0.4.6",
    "@solana/web3.js": "^1.87.6",
    "axios": "^1.6.2",
    "borsh": "^0.7.0",
    "bs58": "^1.2.1",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jito-ts": "^3.0.1",
    "node-fetch": "^3.3.2",
    "socket.io": "^4.7.4",
    "solana": "^3.0.0",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@types/bs58": "^4.0.4",
    "@types/cors": "^2.8.17",
    "@types/crypto-js": "^4.2.1",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.4",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "solana",
    "jito",
    "bundler",
    "mev-protection",
    "defi"
  ],
  "author": "Jito Bundler Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}
